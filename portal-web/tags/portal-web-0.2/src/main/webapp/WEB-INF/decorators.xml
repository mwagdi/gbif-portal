<decorators defaultdir="/WEB-INF/decorator">
  <excludes>
    <pattern>/config-browser/*</pattern>
    <!-- This is temporal. Will be done in 1 or 2 lines with wildcards -->
    <pattern>/admin/*/add/contact*</pattern>
    <pattern>/admin/*/add/tag*</pattern>
    <pattern>/admin/*/add/identifier*</pattern>
    <pattern>/admin/*/add/endpoint*</pattern>  
    <pattern>/admin/*/delete/*</pattern>   
    <pattern>/admin/*/edit/*</pattern>       
  </excludes>
  <decorator name="default" page="default.ftl">
    <pattern>/*</pattern>
  </decorator>
</decorators>
