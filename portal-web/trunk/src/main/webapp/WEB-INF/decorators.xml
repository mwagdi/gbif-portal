<decorators defaultdir="/WEB-INF/decorator">
  <excludes>
    <pattern>/config-browser/*</pattern>
  </excludes>
  <decorator name="dataset-register" page="dataset_register.ftl">
    <pattern>/dataset/register*</pattern>
  </decorator>
  <decorator name="user-register" page="user_register.ftl">
    <pattern>/user/register/*</pattern>
  </decorator>
  <decorator name="default" page="default.ftl">
    <pattern>/*</pattern>
  </decorator>
</decorators>
